{
  "Tasks": [
    {
      "Name": "Codegen",
      "Steps": [
        {
          "Name": "Run",
          "Arguments": [
            "codegen",
            "--clean_before_generate",
            "-R",
            "../../build/dependencies/schema",
            "-i",
            "../../schema",
            "-o",
            "generated",
            "-L",
            "scala"
          ]
        }
      ]
    },
    {
      "Name": "Build",
      "Steps": [
        {
          "Name": "scala",
          "Arguments": [
            "invoke",
            "sbt",
            "pack"
          ]
        }
      ]
    },
    {
      "Name": "Clean",
      "Steps": [
        {
          "Name": "Scala",
          "Command": "sbt",
          "Arguments": [
            "clean"
          ]
        },
        {
          "Name": "Generated code",
          "Arguments": [
            "file",
            "rm",
            "-r",
            "generated"
          ]
        }
      ]
    }
  ]
}